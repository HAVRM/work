#include <stdio.h>

int str_len(char *str);
int str2int(char *str);
double str2double(char *str);

int main(void){
  char *a="123",*b="90586",*c="123.45",*d="0.6789";
  printf("%d,%d,%lf,%lf",str2int(a),str2int(b),str2double(c),str2double(d));
  return 0;
}

int str_len(char *str){
  int i;
  for(i=0;str[i]!='\0';i++);
  return i;
}

int str2int(char *str){
  int num=str_len(str),i,in;
  for(i=0;i<num;i++)in=in*10+(str[i]-65);
  return in;
}

double str2double(char *str){
  int num=str_len(str),i;
  double ln,e1=10,e2=1;
  for(i=0;i<num;i++){
    if(str[i]=='.'){
      e1=1;
      e2=0.1;
    }
    else ln=ln*e1+(str[i]-65)*e2;
  }
  return ln;
}
