#ifndef SHOUGI_CHANGE_H
#define SHOUGI_CHANGE_H

int return_1(int num){return num-((int)(num/10))*10;}

void set_space(int* s,int x,int y){
  int i,j,k;
  for(i=0;i<20;i++){
    for(j=0;j<2;j++){
      k=s[i*2+j];
      if(k<0 || k>8){
	s[i*2]=x-1;
	s[i*2+1]=y-1;
	j=1;
      }
    }
  }
}

